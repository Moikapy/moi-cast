{
  "name": "moi-cast",
  "productName": "Moi Cast",
  "description": "Video Creation Tool",
  "keywords": [],
  "main": "main.js",
  "version": "1.0.0",
  "author": "leftp",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4"
  },
  "devDependencies": {

    "@electron-forge/cli": "6.0.0-beta.34",
    "@electron-forge/maker-deb": "6.0.0-beta.34",
    "@electron-forge/maker-rpm": "6.0.0-beta.34",
    "@electron-forge/maker-squirrel": "6.0.0-beta.34",
    "@electron-forge/maker-zip": "6.0.0-beta.34",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.34",
    "electron": "5.0.8",
    "yarn": "^1.17.3"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel"
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        },
		{
		  "plugins": [
			["@electron-forge/plugin-webpack", {
			 "mainConfig": "./webpack.main.config.js",
			  "renderer": {
				"entryPoints": [{
				  "html": "./public/index.html",
				  "js": "./src/renderer/index.js",
				  "name": "main_window"
				}]
			  }
			}]
		  ]
		}
      ]
    }
  }
}
